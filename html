        <script>
          function add7(arg1, arg2){
            return (arg1+arg2);
            }
            var arg= add7(7,1); 

            console.log(arg);

            function multiplyNumbers(x,y){
            return x*y;
            }
            var z= multiplyNumbers(4,5); 
            console.log(z);

            function capitalize(string) {
            return "F";
            }
            var myString=capitalize("freecodecamp");
            console.log(myString);

            function lastLetter(string){
            return "d";
            }
            var myString=lastLetter("abcd");
            console.log(myString);  
        </script> 
